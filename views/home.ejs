<%- include("partials/header"); -%>

 <%  if(startingContent.length<1){  %> 
<h1> <%=defaultpost.title %> </h1>
<p><%= defaultpost.content %></p>
   <%  }  %>

<% for(let i = 0;i<startingContent.length;i++) { %>

<h1> <%= startingContent[i].title %> </h1>
<p><%= startingContent[i].content.substring(0,50) + " ..."%><a href="<%= "/post/"+ startingContent[i]._id %>"> Read More</a></p>

<% } %>
<%- include("partials/footer"); -%>